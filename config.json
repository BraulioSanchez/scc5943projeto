{
	"data": {
		"symbol": "AMZN",
		"columns": [
			"Close"
		],
		"start": "2008-04-01",
		"end": "2019-10-01",
		"sequence_length": 5,
		"train_test_split": 0.85,
		"save_dir": "data",
		"correlates_to": [
			"NOV",
			"EQIX",
			"CTAS",
			"CRM",
			"BSX",
			"IT",
			"V",
			"MSI",
			"CLX",
			"WAT"
		]
	},
	"preprocessing": {
		"denoise": {
			"wavelet": "haar",
			"thr_mode": "hard"
		},
		"filename": "preprocessed.csv",
		"save_dir": "preprocessing"
	},
	"training": {
		"epochs": 2,
		"batch_size": 32
	},
	"model": {
		"loss": "mae",
		"optimizer": "adam",
		"save_dir": "model",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"activation": "relu",
				"input_timesteps": 4,
				"input_dim": 1,
				"return_sequences": false
			},
			{
				"type": "repeat_vector",
				"output_shape": 1
			},
			{
				"type": "lstm",
				"neurons": 100,
				"activation": "relu",
				"return_sequences": true
			},
			{
				"type": "time_distributed",
				"layer": {
					"type": "dense",
					"neurons": 1
				}
			}
		]
	}
}
